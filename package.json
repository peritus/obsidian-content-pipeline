{
  "name": "obsidian-audio-inbox",
  "version": "1.0.0",
  "description": "A sophisticated audio inbox management plugin for Obsidian that helps organize and process audio recordings efficiently",
  "main": "main.js",
  "scripts": {
    "build": "node esbuild.config.js",
    "build:debug": "OBSIDIAN_AUDIO_INBOX_LOGLEVEL=debug node esbuild.config.js",
    "build:info": "OBSIDIAN_AUDIO_INBOX_LOGLEVEL=info node esbuild.config.js",
    "build:warn": "OBSIDIAN_AUDIO_INBOX_LOGLEVEL=warn node esbuild.config.js",
    "build:error": "OBSIDIAN_AUDIO_INBOX_LOGLEVEL=error node esbuild.config.js",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix"
  },
  "keywords": [
    "obsidian",
    "obsidian-plugin",
    "audio",
    "voice-notes",
    "recording",
    "inbox",
    "productivity",
    "note-taking",
    "audio-management",
    "workflow",
    "organization"
  ],
  "author": {
    "name": "Filip",
    "email": "your.email@example.com",
    "url": "https://github.com/yourusername"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/obsidian-audio-inbox.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/obsidian-audio-inbox/issues"
  },
  "homepage": "https://github.com/yourusername/obsidian-audio-inbox#readme",
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "main.js",
    "styles.css",
    "manifest.json",
    "LICENSE",
    "README.md"
  ],
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "builtin-modules": "^3.3.0",
    "esbuild": "^0.19.12",
    "eslint": "^8.56.0",
    "obsidian": "^1.4.16",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {},
  "peerDependencies": {
    "obsidian": "^1.0.0"
  }
}