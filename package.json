{
  "name": "obsidian-audio-inbox",
  "version": "1.0.0",
  "description": "A sophisticated audio inbox management plugin for Obsidian that helps organize and process audio recordings efficiently",
  "main": "main.js",
  "scripts": {
    "build": "NODE_ENV=production node esbuild.config.js",
    "build-debug": "LOGGING_ENABLED=true node esbuild.config.js",
    "build:watch": "node esbuild.config.js --watch",
    "build:dev": "NODE_ENV=development node esbuild.config.js",
    "build:dev:watch": "NODE_ENV=development node esbuild.config.js --watch",
    "build:debug:watch": "LOGGING_ENABLED=true node esbuild.config.js --watch",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "dev": "npm run build:debug:watch",
    "clean": "rm -f main.js main.js.map",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "prebuild": "npm run type-check",
    "predev": "npm run clean",
    "version": "npm run build && git add -A",
    "postversion": "git push && git push --tags",
    "release": "npm version patch && npm run build",
    "release:minor": "npm version minor && npm run build",
    "release:major": "npm version major && npm run build"
  },
  "keywords": [
    "obsidian",
    "obsidian-plugin",
    "audio",
    "voice-notes",
    "recording",
    "inbox",
    "productivity",
    "note-taking",
    "audio-management",
    "workflow",
    "organization"
  ],
  "author": {
    "name": "Filip",
    "email": "your.email@example.com",
    "url": "https://github.com/yourusername"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/obsidian-audio-inbox.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/obsidian-audio-inbox/issues"
  },
  "homepage": "https://github.com/yourusername/obsidian-audio-inbox#readme",
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "main.js",
    "styles.css",
    "manifest.json",
    "README.md"
  ],
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "builtin-modules": "^3.3.0",
    "esbuild": "^0.19.12",
    "eslint": "^8.56.0",
    "obsidian": "^1.4.16",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {},
  "peerDependencies": {
    "obsidian": "^1.0.0"
  }
}